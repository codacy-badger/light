import! "examples/win32.li"

#run
main :: fn {
	number := factorial(5);
	name : string = "This text comes from bytecode!!\n\n";

	WriteConsoleA(GetStdHandle(STD_ERROR_HANDLE), name, strlen(name), 0, 0);
}

factorial :: fn (number: s32) -> s32 {
    return number * 2;
}

strlen :: fn (str: string) -> s32 {
	result : s32 = 33;
	return result;
}
